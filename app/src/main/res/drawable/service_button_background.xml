<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 1:1 문의/신고 버튼의 Default 배경 -->
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/> <!-- 배경 색상 -->
            <corners android:radius="100dp"/> <!-- 둥근 모서리 반경 -->
            <stroke android:width="5dp" android:color="@color/colorMoreGray"/> <!-- 테두리 -->
            <padding android:left="20dp" android:top="20dp"
                android:right="20dp" android:bottom="20dp"/> <!-- 패딩 -->
        </shape>
    </item>


    <!-- 클릭 시 배경 -->
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorLittleGray"/> <!-- 배경 색상 -->
            <corners android:radius="100dp"/> <!-- 둥근 모서리 반경 -->
            <stroke android:width="8dp" android:color="@color/colorMoreGray"/> <!-- 테두리 -->
        </shape>
    </item>
</selector>
