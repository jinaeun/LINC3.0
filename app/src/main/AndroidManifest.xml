        android:supportsRtl="true"
        android:theme="@style/Theme.LINC_SADAJU"
        tools:targetApi="34">

        <activity
            android:name=".mypage.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:textColor="@color/black">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".mypage.MyPage_RecentSeenProduct"
            android:exported="true" />
        <activity
            android:name=".mypage.MyPage_ForCustomer"
            android:exported="true" />
        <activity
            android:name=".mypage.MyPage_Post"
            android:exported="true" />
        <activity
            android:name=".mypage.MyPage_Editor"
            android:exported="true" />
        <activity
            android:name=".mypage.MyPage_Trade"
            android:exported="true" />
        <activity
            android:name=".mypage.MyPage_OnebyoneInquiry"
            android:exported="true" />
        <activity
            android:name=".mypage.MyPage"
            android:exported="true" />
    </application>

</manifest>